menu id MENU_main
begin
  pulldown "Uni Mensa"
  begin
    menuitem "men_beam" id MENUITEM_beam "B"
    menuitem separator
    menuitem "men_ab" id MENUITEM_about "A"
  end
  pulldown "men_opt"
  begin
    menuitem "men_prefs" id MENUITEM_prefs "P"
    menuitem separator
    menuitem "men_dbinfo" id MENUITEM_dbinfo "I"
  end
end


alert id ALERT_ROMIncompatible
